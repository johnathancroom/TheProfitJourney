<% content_for :body_content_title do %>
  <%= "#{page_title} Technicians" %>
<% end %>

<% content_for :body do %>
  <%= link_to "Add Technician", refinery.new_akouo_journeyboard_technician_path %>

  <hr>

  <ul>
    <% if @technicians.blank? %>
      <li>No Technicians Yet</li>
    <% else %>
      <% @technicians.each do |technician| %>
        <li>
          <%= technician.name %>
          <%= link_to "View", refinery.akouo_journeyboard_technician_path(technician) %>
          <%= link_to "Edit", refinery.edit_akouo_journeyboard_technician_path(technician) %>
        </li>
      <% end %>
    <% end %>
  </ul>
<% end %>

<%= render "/refinery/content_page" %>