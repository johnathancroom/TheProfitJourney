<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(customer) -%>">
  <span class='title'>
    <%= customer.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.akouo_customer_path(customer),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_akouo_admin_customer_path(customer),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.akouo_admin_customers_path(customer),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => customer),
        :method => :delete %>
  </span>
</li>
